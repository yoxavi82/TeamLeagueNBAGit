<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.teamleaguebagit">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name="com.example.teamleaguebagit.MenuBottom.MenuBottomMercado"></activity>
        <activity android:name="com.example.teamleaguebagit.Mercado" />
        <activity android:name="com.example.teamleaguebagit.MenuBottom.MenuBottomAlineacion" />
        <activity android:name="com.example.teamleaguebagit.Alineacion" />
        <activity android:name="com.example.teamleaguebagit.MenuBottom.MenuBottomJornada" />
        <activity android:name="com.example.teamleaguebagit.Jornada" />
        <activity android:name="com.example.teamleaguebagit.Clasificacion" />
        <activity
            android:name="com.example.teamleaguebagit.ranking.Ranking"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.teamleaguebagit.Homepage"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.teamleaguebagit.RegisterView"
            android:screenOrientation="portrait" />
        <activity
            android:name="com.example.teamleaguebagit.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>